@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":AddExpenseCommandParser" as AddExpenseCommandParser LOGIC_COLOR
participant "addExpenseCommand:AddExpenseCommand" as AddExpenseCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("addexpense n/Alex Yeoh gn/Bali $/50 d/Happy hour n/Bernice Yu $/10 ex/Charlie Tan")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("addexpense n/Alex Yeoh gn/Bali $/50 d/Happy hour n/Bernice Yu $/10 ex/Charlie Tan")
activate AddressBookParser

create AddExpenseCommandParser
AddressBookParser -> AddExpenseCommandParser
activate AddExpenseCommandParser

AddExpenseCommandParser --> AddressBookParser
deactivate AddExpenseCommandParser

AddressBookParser -> AddExpenseCommandParser : parse("n/Alex Yeoh gn/Bali $/50 d/Happy hour n/Bernice Yu $/10 ex/Charlie Tan")
activate AddExpenseCommandParser

create AddExpenseCommand
AddExpenseCommandParser -> AddExpenseCommand
activate AddExpenseCommand

AddExpenseCommand --> AddExpenseCommandParser : addExpenseCommand
deactivate AddExpenseCommand

AddExpenseCommandParser --> AddressBookParser : addExpenseCommand
deactivate AddExpenseCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
AddExpenseCommandParser -[hidden]-> AddressBookParser
destroy AddExpenseCommandParser

AddressBookParser --> LogicManager : addExpenseCommand
deactivate AddressBookParser

LogicManager -> AddExpenseCommand : execute()
activate AddExpenseCommand

AddExpenseCommand -> Model : addExpense(e:Expense, g:Group)
activate Model

Model --> AddExpenseCommand
deactivate Model

create CommandResult
AddExpenseCommand -> CommandResult
activate CommandResult

CommandResult --> AddExpenseCommand
deactivate CommandResult

AddExpenseCommand --> LogicManager : result
deactivate AddExpenseCommand

[<--LogicManager
deactivate LogicManager
@enduml
