@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AweParser" as AweParser LOGIC_COLOR
participant ":DeleteExpenseCommand" as DeleteExpenseCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("deleteexpense gn/Bali n/Tom")
activate LogicManager

LogicManager -> AweParser : parseCommand("deleteexpense gn/Bali n/Tom")
activate AweParser

/'Clarify with tutor if there is a need to display DeleteExpenseCommandParser class within the ref'/
ref over AweParser, DeleteExpenseCommand:get createGroupCommand
create DeleteExpenseCommand
AweParser -> DeleteExpenseCommand


AweParser --> LogicManager : createGroupCommand
deactivate AweParser

LogicManager -> DeleteExpenseCommand : execute()
activate DeleteExpenseCommand

DeleteExpenseCommand -> Model : DeleteExpense(g:Group)
activate Model

Model --> DeleteExpenseCommand
deactivate Model

create CommandResult
DeleteExpenseCommand -> CommandResult
activate CommandResult

CommandResult --> DeleteExpenseCommand
deactivate CommandResult

DeleteExpenseCommand --> LogicManager : result
deactivate DeleteExpenseCommand

[<--LogicManager
deactivate LogicManager
@enduml